  <DataSets>
    <DataSet Name="RoomsBelowGivenPrice">
      <Query>
        <DataSourceName>Pickle_FARMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@maxRackRate">
            <Value>=Parameters!maxRackRate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT HotelName 'Hotel Name', '$' + CONVERT(varchar(12), RackRate) 'Rack Rate', RoomNumber 'Room Number', RoomDescription 'Room Description', RoomSmoking 'Smoking', RoomBedConfiguration 'Bed Configuration'
FROM Hotel h 
JOIN Room r ON r.HotelID=h.HotelID
JOIN RoomType rt ON rt.RoomTypeID=r.RoomTypeID
JOIN RackRate rr ON rr.RoomTypeID=rt.RoomTypeID
WHERE RackRate &lt;= @maxRackRate
AND DATEPART(m, RackRateBegin) &lt;= 6
AND DATEPART(m, RackRateEnd) &gt;= 6 
ORDER BY RackRate ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="Hotel_Name">
          <DataField>Hotel Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Room_Number">
          <DataField>Room Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Room_Description">
          <DataField>Room Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rack_Rate">
          <DataField>Rack Rate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Smoking">
          <DataField>Smoking</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="Bed_Configuration">
          <DataField>Bed Configuration</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
