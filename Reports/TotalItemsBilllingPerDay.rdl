  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Pickle_FARMS</DataSourceName>
        <CommandText>SELECT SUM(BillingAmount) 'Item Billing Amount', BillingItemDate
FROM Billing b
JOIN Folio f
ON b.FolioID = f.FolioID
JOIN Room r
ON f.RoomID = r.RoomID
JOIN Hotel h
ON r.HotelID = h.HotelID
GROUP BY BillingItemDate</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_Billing_Amount">
          <DataField>Item Billing Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BillingItemDate">
          <DataField>BillingItemDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
