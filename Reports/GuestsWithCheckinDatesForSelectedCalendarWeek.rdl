  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Pickle_FARMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@targetDate">
            <Value>=Parameters!targetDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT GuestFirst 'First Name', GuestLast 'Last Name', CONVERT(varchar(20), CheckinDate, 107) 'Checkin Date', HotelName 'Hotel Name', RoomNumber 'Room Number', GuestPhone 'Guest''s Phone'
FROM Guest g
JOIN CreditCard cc
ON g.GuestID = cc.GuestID
JOIN Reservation r
ON cc.CreditCardID = r.CreditCardID
JOIN Folio f
ON r.ReservationID = f.ReservationID
JOIN Room rm
ON f.RoomID = rm.RoomID
JOIN Hotel h
ON rm.HotelID = h.HotelID
WHERE DATEPART(WW, CheckinDate) = DATEPART(WW, @targetDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="First_Name">
          <DataField>First Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
